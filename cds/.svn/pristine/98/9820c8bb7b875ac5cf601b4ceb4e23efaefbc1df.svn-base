/*--------------------select-box css---------------*/
.sbHolder {
	background-color: #fff;
	border: solid 1px #cccccc !important;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 20px !important;
	font-style: normal;
	color: #000;
	font-weight: normal;
	height: 30px;
	position: relative;
	width: 600px;
	border-right: 0px none;
	/* z-index: 999; */
}

.sbHolder a {
	font-size: 15px !important;
	color: #000 !important;
	text-transform: uppercase;
	font-weight: normal;
	background-color: white;
}

.sbHolder:focus .drop_box .sbSelector {
	
}

.sbSelector {
	display: block;
	height: 30px;
	left: 0;
	line-height: 36px;
	outline: none;
	overflow: hidden;
	position: absolute;
	text-indent: 12px;
	top: 0;
	width: 100%;
}

.sbSelector:link, .drop_box .sbSelector:visited, .drop_box .sbSelector:hover
	{
	color: #000;
	outline: none;
	text-decoration: none;
	text-transform: inherit;
	font-size: 17px;
}

.sbToggle {
	background: url(../images/select_bulet.png) right 0 no-repeat;
	display: block;
	height: 9px;
	outline: none;
	position: absolute;
	right: 7px;
	top: 15px;
	width: 8px;
}

.sbToggle:hover {
	background: url(../images/select_bulet.png) right 0 no-repeat;
}

.sbToggleOpen {
	background: url(../images/select_bulet_h.png) right 0 no-repeat;
}

.sbToggleOpen:hover {
	background: url(../images/select_bulet_h.png) right 0 no-repeat;
}

.sbHolderDisabled {
	background-color: #fff;
	border: solid 1px #eaeaea;
}

.sbHolderDisabled .sbHolder {
	
}

.sbHolderDisabled .sbToggle {
	
}

.sbOptions {
	background-color: #fff;
	border: solid 1px #eaeaea;
	list-style: none;
	left: -1px;
	margin: 0;
	padding: 0;
	position: absolute;
	top: 39px !important;
	width: 100%;
	z-index: 1;
	overflow-y: auto;
	-webkit-border-bottom-right-radius: 6px;
	-webkit-border-bottom-left-radius: 6px;
	-moz-border-radius-bottomright: 6px;
	-moz-border-radius-bottomleft: 6px;
	border-bottom-right-radius: 6px;
	border-bottom-left-radius: 6px;
	height:500px;
}

.sbOptions li {
	padding: 0 0;
}

.sbOptions a {
	display: block;
	outline: none;
	padding: 4px 0 4px 8px;
}

.sbOptions a:link, .drop_box .sbOptions a:visited {
	color: #000;
	text-decoration: none;
	text-transform: inherit;
	font-size: 17px;
}

.sbOptions a:hover, .sbOptions a:focus, .sbOptions a.sbFocus {
	color: #000;
}

.sbOptions li.last a {
	border-bottom: none;
}

.sbOptions .sbDisabled {
	color: #000;
	display: block;
	padding: 7px 0 7px 3px;
}

.sbOptions .sbGroup {
	border-bottom: dotted 1px #515151;
	color: #EBB52D;
	display: block;
	font-weight: bold;
	padding: 7px 0 7px 3px;
}

.sbOptions .sbSub {
	padding-left: 17px;
}

