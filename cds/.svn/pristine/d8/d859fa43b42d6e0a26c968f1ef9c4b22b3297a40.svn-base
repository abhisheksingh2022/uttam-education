<%-- Method used to set the TOTAL COST for each plan on page load as well as at the stop of slider.  --%>
function setCalculatedTotal(object, newAPTC){
	if(getInteger(object.attr("minAmount")) < (getInteger(object.attr("netPremium")) - newAPTC)){
		amt = getInteger(object.attr("netPremium")) - newAPTC;
		amt = parseFloat(Math.round((amt + parseFloat(getInteger(object.attr("outOfPocketMax"))))*100)/100).toFixed(2);
		object.text("$"+amt);
	}else{
		amt= parseFloat(getInteger(object.attr("minAmount"))) + parseFloat(getInteger(object.attr("outOfPocketMax")));
		object.text("$"+amt.toFixed(2));
	}

}

function setCalculatedAmount(object, newAPTC){
	  var min = parseFloat(object.attr("minAmount"));
	  var net = parseFloat(object.attr("netPremium"));
	  var amt = "";
	  if( min < (net - newAPTC)){
	   amt = net - newAPTC;
	   
	  }else{
	   amt = min;
	   
	  }
	  object.text("$"+amt.toFixed(2));
	}

<%-- Method used to get the TOTAL COST for each plan for creating theCOST BAR for wach plan.  --%>
function getCalculatedTotal(object, newAPTC){
	if(parseFloat(getInteger(object.attr("minAmount"))) < parseFloat(getInteger(object.attr("netPremium")) - newAPTC)){
		amt = getInteger(object.attr("netPremium")) - newAPTC;
		amt = amt+ parseFloat(getInteger(object.attr("outOfPocketMax")));
		return amt;
	}else{
		amt= parseFloat(getInteger(object.attr("minAmount"))) + parseFloat(getInteger(object.attr("outOfPocketMax")));
		return amt;
	}

}

<%-- Method used to get the MINIMUM PREMIUM COST from the list of AMOUNTS.  --%>
function minFromList(object){
	var min;
	var newLoop=0;
	object.each(function() {
		if(newLoop==0){
			newLoop=1;
			min=getInteger($(this).text());
		}
		if(parseFloat(getInteger($(this).text())) < parseFloat(min)){
			min=getInteger($(this).text());
		}
	});
	return min;
}

<%-- Method used to get the MAXIMUM PREMIUM COST from the list of AMOUNTS.  --%>
function maxFromList(object){
	var max;
	var newLoop=0;
	object.each(function() {
		if(newLoop==0){
			newLoop=1;
			max=getInteger($(this).text());
		}
		if(parseFloat(getInteger($(this).text()))> parseFloat(max)){
			max=getInteger($(this).text());
		}
	});
	return max;
}

<%-- Method for setting the width and height the LEFT BAR(ORANGE BAR) --%>
function drawImageLeft(total, maxPremium, outOfPocketMax,element){
	width = barTotalWidth;	// FIXED TO 175 for alignment of BAR and TOTAL COST in one element.
	outOfPocketMax = getInteger(outOfPocketMax); //Remove '$' from outOfPocketMax
	<%-- 
		 1. Get the total width that the specific plan can take. For the plan with 
			maximum amount this would be equal to the barTotalWidth.
		 2. Get the Right Cordinate of the specific plan. For the plan with maximum
	 		amount this would be equal to the barTotalWidth.
	 	 3. Get premium calculated from TOTAL COST and OUT OF POCKET COST.	
	 	 4. Get Mid Cordinate by taking the percent of PREMIUM AMOUNT from the TOTAL 
	 	 	COST and taking that percent from the Right Cordinate. 
	 	 5. Setting the width of the left bar from 0 to Mid Cordinate.
	--%>
	width = width * (total/maxPremium);
	var rightCordinate = width *(total / maxPremium) ;
	var premium = total-outOfPocketMax;
	var midCordinate = rightCordinate * (premium / total);
	element.width = midCordinate;
	element.height = barHeight;
	
}

<%-- Method for setting the width and height the RIGHT BAR(GREEN BAR) --%>
function drawImageRight(total, maxPremium, outOfPocketMax,element){
	width = barTotalWidth;	// FIXED TO 175 for alignment of BAR and TOTAL COST in one element.
	outOfPocketMax = getInteger(outOfPocketMax);  //Remove '$' from outOfPocketMax
	<%-- 
	 1. Get the total width that the specific plan can take. For the plan with 
		maximum amount this would be equal to the barTotalWidth.
	 2. Get the Right Cordinate of the specific plan. For the plan with maximum
		amount this would be equal to the barTotalWidth.
	 3. Get premium calculated from TOTAL COST and OUT OF POCKET COST.	
	 4. Get Mid Cordinate by taking the percent of PREMIUM AMOUNT from the TOTAL 
	 	COST and taking that percent from the Right Cordinate.
	 5. Setting the element width to start from where the left bar ended. 
--%>
	width = width * (total/maxPremium);
	var rightCordinate = width *(total / maxPremium) ;
	var premium = total-outOfPocketMax;
	var midCordinate = rightCordinate * (premium / total);
	element.width = (rightCordinate-midCordinate);
	element.height = barHeight;
}

<%-- 
	Method for drawing the slider. The VALUE FOR SLIDER variable will have the values according to selection of MONTHLY/YEARLY value.
	and the SELECTED VALUE attribute will have the value that is to be selected by default.
--%>
function drawSlider(valueForSlider, selectedValue){
	$("#slider-range-min").slider({
		value :selectedValue ,
		min : sliderMinValue,
		max : valueForSlider[valueForSlider.length -1],
		slide : function(event, ui) {
				$("#amount").val("$" + parseFloat(valueForSlider[ui.value]).toFixed(2));
			}
	});
}

