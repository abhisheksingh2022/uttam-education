				$(this).text("$" + parseFloat(Math.round(parseFloat(getInteger($(this).text()))/convertToYearlyNumber*100)/100).toFixed(2));
			});
			$("#amount").val("$" + parseFloat(Math.round(parseFloat(getInteger($("#amount").val()))/12*100)/100).toFixed(2));
			//alert("M " + Math.floor($("#slider-range-min").slider("value")/12) + " " + Math.round(getInteger($("#amount").val())));
			drawSlider(valueForSlider, valueForSlider[ Math.round(getInteger($("#amount").val()))]);
			flagMY=0;
			
		}else if($(".myRadio:checked").val() == "Y" && flagMY == 0){
			$(".changeMonthlyYearly").each(function(){
				$(this).text("$" + parseFloat(Math.round(parseFloat(getInteger($(this).text()))*convertToYearlyNumber*100)/100).toFixed(2));
			});
			$(".changeMonthlyYearlyEstAmt").each(function(){
				$(this).text("$" + parseFloat(Math.round(parseFloat(getInteger($(this).text()))*convertToYearlyNumber*100)/100).toFixed(2));
			});
			$("#amount").val("$" + parseFloat(Math.round(parseFloat(getInteger($("#amount").val()))*12*100)/100).toFixed(2));
			//alert("Y " + $("#slider-range-min").slider("value")+ " " + Math.round((getInteger($("#amount").val())/12))    );
			drawSlider(valueForSliderYearly, valueForSliderYearly[ Math.round((getInteger($("#amount").val())/12))]*12);
			flagMY=1;
		}
} 	
</script>

