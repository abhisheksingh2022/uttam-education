  <xsd:element abstract="false" name="InsuranceCompanyReinsuranceContact" substitutionGroup="hix-pm:InsuranceCompanyContact" type="hix-core:PersonType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:Base i:name="InsuranceCompanyContact" i:namespace="http://hix.cms.gov/0.1/hix-pm"/>
      </xsd:appinfo>
