          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyNAICGroupID"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:RoleOfOrganizationReference"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
