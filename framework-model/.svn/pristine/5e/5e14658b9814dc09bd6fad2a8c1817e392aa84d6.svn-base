          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyComplaintTrackingContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyQualityContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyComplianceOfficerContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyAptcCsrContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyRiskCorridorsContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyUserAccessPrimaryContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyUserAccessSecondaryContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyChiefDentalDirectorContact"/>
