    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="InsuranceCompanyUserAccessSecondaryContact" substitutionGroup="hix-pm:InsuranceCompanyContact" type="hix-core:PersonType">
    <xsd:annotation>
