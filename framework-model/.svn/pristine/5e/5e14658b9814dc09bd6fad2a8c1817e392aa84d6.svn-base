  <xsd:element abstract="false" name="InsuranceCompanyTIN-ID" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation source="">An identifier assigned by the Internal Revenue Service (IRS) for the purpose of tax filing.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="InsuranceCompanyNAIC-ID" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation source="">An identifier assigned by the National Association of Insurance Commissioners (NAIC) to a US domiciled company that files an annual finanical statement with the NAIC.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="InsuranceCompanyNAICGroupID" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation source="">An identifier assigned by the National Association of Insurance Commissioners (NAIC) to identify a group of companies within a holding company.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="RoleOfOrganizationReference" nillable="false" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="OrganizationType" i:namespace="http://hix.cms.gov/0.1/hix-core"/>
      </xsd:appinfo>
      <xsd:documentation source="">A reference to information about the issuer's organization. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType abstract="false" mixed="false" name="IssuerType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
      </xsd:appinfo>
      <xsd:documentation source="">A data type for a licensed health insurance issuing organization registered in a specific state.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" ref="hix-pm:IssuerIndividualMarketContact"/>
          <xsd:element maxOccurs="1" minOccurs="0" ref="hix-pm:IssuerSmallGroupMarketContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerIdentification"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerStateCode"/>
          <xsd:element maxOccurs="1" minOccurs="0" ref="hix-pm:IssuerSHOPCustomerService"/>
          <xsd:element maxOccurs="1" minOccurs="0" ref="hix-pm:IssuerIndividualMarketCustomerService"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:RoleOfOrganizationReference"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerEnrollmentThirdPartyAdministrationIndicator"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerClaimsProcessingThirdPartyAdministrationIndicator"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerEdgeServerHostThirdPartyAdministrationIndicator"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerCurrentSalesMarketCode"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:IssuerProposedExchangeMarketCoverageCode"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="false" name="IssuerIndividualMarketContact" nillable="false" type="hix-core:PersonType">
    <xsd:annotation>
      <xsd:documentation source="">A contact person in charge of the individual market at the issuer level.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerSmallGroupMarketContact" nillable="false" type="hix-core:PersonType">
    <xsd:annotation>
      <xsd:documentation source="">A contact person in charge of the small group market at the issuer level.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerIdentification" nillable="false" type="nc:IdentificationType">
    <xsd:annotation>
      <xsd:documentation source="">An identifier for the issuer.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerStateCode" nillable="false" type="usps:USStateCodeType">
    <xsd:annotation>
      <xsd:documentation source="">A state where the issuer is authorized to do business.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerSHOPCustomerService" nillable="false" type="nc:ContactInformationType">
    <xsd:annotation>
      <xsd:documentation source="">A customer service contact for the Small Business Health Options Program (SHOP) at the issuer level.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerIndividualMarketCustomerService" nillable="false" type="nc:ContactInformationType">
    <xsd:annotation>
      <xsd:documentation source="">A customer service contact for the individual market at the issuer level.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerEnrollmentThirdPartyAdministrationIndicator" nillable="false" type="niem-xsd:boolean">
    <xsd:annotation>
      <xsd:documentation source="">True if the issuer organization has third party administrator organizations for enrollment processing; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerClaimsProcessingThirdPartyAdministrationIndicator" nillable="false" type="niem-xsd:boolean">
    <xsd:annotation>
      <xsd:documentation source="">True if the issuer organization has third party administrator organizations for claims processing; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerEdgeServerHostThirdPartyAdministrationIndicator" nillable="false" type="niem-xsd:boolean">
    <xsd:annotation>
      <xsd:documentation source="">True if the issuer organization has third party administrator organizations for edge server mart processing; false otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerCurrentSalesMarketCode" nillable="false" substitutionGroup="hix-pm:IssuerMarketCoverageCode" type="hix-pm:InsurancePlanLevelCodeType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:Base i:name="IssuerMarketCoverageCode" i:namespace="http://hix.cms.gov/0.1/hix-pm"/>
      </xsd:appinfo>
      <xsd:documentation source="">A kind of market in which this issuer currently participates.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="IssuerMarketCoverageCode" nillable="false" type="hix-pm:InsurancePlanLevelCodeType">
    <xsd:annotation>
      <xsd:documentation source="">A kind of market in which this issuer participates.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType abstract="false" mixed="false" name="InsurancePlanLevelCodeType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
      </xsd:appinfo>
      <xsd:documentation source="">A data type for a code indicating different values of market coverage that an issuer can have. Example include Individual, SHOP (for small business), Both.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="hix-pm:InsurancePlanLevelCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="InsurancePlanLevelCodeSimpleType">
    <xsd:annotation>
      <xsd:appinfo>
        <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
      </xsd:appinfo>
      <xsd:documentation source="">A data type for a code indicating different values of market coverage that an issuer can have. Example include Individual, SHOP (for small business), Both.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Individual">
        <xsd:annotation>
          <xsd:documentation source="">A value indicating the market coverage for individuals.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SHOP">
        <xsd:annotation>
          <xsd:documentation source="">A value indicating the market coverage for small businesses</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Both">
        <xsd:annotation>
          <xsd:documentation source="">A value indicating the market coverage for both individuals and small businesses.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element abstract="false" name="IssuerProposedExchangeMarketCoverageCode" nillable="false" substitutionGroup="hix-pm:IssuerMarketCoverageCode" type="hix-pm:InsurancePlanLevelCodeType">
