        <i:Base i:name="InsuranceCompanyContact" i:namespace="http://hix.cms.gov/0.1/hix-pm"/>
      </xsd:appinfo>
      <xsd:documentation source="">An individual who is responsible for managing pharmacy benefits.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="InsuranceCompanyFinancialTransfersContact" substitutionGroup="hix-pm:InsuranceCompanyContact" type="hix-core:PersonType">
    <xsd:annotation>
