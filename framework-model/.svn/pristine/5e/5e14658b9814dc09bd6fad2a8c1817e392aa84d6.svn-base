          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyMedicalDirectorContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyPaymentContact"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="hix-pm:InsuranceCompanyGovernmentRelationsContact"/>
