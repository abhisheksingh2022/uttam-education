.switch-tabs {
	display: table-cell;
	vertical-align: middle;
	text-align: center
}

.width100 {
	width: 100px;
}

textarea {
	resize: none;
	font-size: 12px !important;
}

.benefits-sec-Table2 tbody {
	display: inline-table;
	width: 100%;
}

.benefits-sec-Table2 tr td:last-child {
	border-right: 1px solid #ccc;
}

.header-table-td {
	border-top: 1px solid #ccc;
}

.benefits-sec-Table2 tr td {
	text-align: center;
	border-collapse: separate;
	border-left: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	padding: 5px;
	font-size: 13px;
	word-break: normal;
}

.search-text {
	width: 13%;
}

