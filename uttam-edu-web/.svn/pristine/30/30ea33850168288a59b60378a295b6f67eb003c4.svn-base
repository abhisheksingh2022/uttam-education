/*=========================Responsive nav Start===============*/

.toggleMenu {
    display:  none;
    background: #474646;
    padding: 10px 15px;
	color: #FFFFFF;
    font-family: 'robotoregular';
    font-size: 14px;

}
.nav:before,
.nav:after {
    content: " "; 
    display: table; 
}
.nav:after {
    clear: both;
}
.nav ul {
    list-style: none;
    width: 190px;
	background:#ff8d00;
	border-radius:0 0 5px 5px; 
	border-top:1px solid #fff;
	padding:0;
}
.nav a {
   border-radius:5px 5px 0 0;    
}
.nav li {
    position: relative;
	
}
.nav > li {
    float: left;
	width:19.79%; text-align:center; margin-right:2px;
}

.nav > li > a {
    background:#ffb100; display:block; font-size:15px; line-height:45px; font-weight:normal; color:#fff; padding:0; margin:0;
}
.test{ margin-left:30%;}
.nav > li > a:hover{ background:#ff8d00; color:#fff; display:block;}
.nav > li > a:focus{background:#ff8d00; color:#fff;}
.nav > li > a.parent:hover{ background:#F16527; color:#000; display:block;}

.nav li  ul {
    position: absolute;
    left: -9999px;
}
.nav > li.hover > ul {
    left: 0;
}
.nav li li.hover ul {
    left: 100%;
    top: 0;
}
.nav li li{margin-right:0; text-align:left!important; display:block; border-top:1px solid #ffc782; border-bottom:1px solid #e08616;}
.nav li li:first-child{border-top:none;}
.nav li li:last-child{border-bottom:none;}
.nav li li a {
    display: block;
    position: relative;
    z-index:100;
	color: #fff;
    font-size: 14px;
	line-height:40px;
	margin:0;
	padding:0;	
	padding-left:15px;	
}
.nav li li a:hover{ background:#F47226; border-radius:0; text-decoration:none; color:#fff;}
.nav li li li a {    
    z-index:200;
}
.nav li.hover a{background:#F16527; color:#000; border-radius:5px 5px 0 0;}


