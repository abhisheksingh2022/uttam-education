.plan-table {
	margin: 20px 0;
	display: inline-block;
	width: 100%;
}

.plan-table table tr {
	border-bottom: 1px solid #CCC;
}

.plan-table table tr td {
	border: 0px solid #000;
	padding: 5px;
	width: 4%;
	text-align: left;
}

.plan-table table tr:last-child {
	border-bottom: 0px solid #000;
}

.ac-trHd td {
	font-size: 15px;
	color: rgb(235, 87, 49);
	background: #f5f5f5;
	text-align: center;
	font-weight: 700;
	height: 50px;
}

.plan-table-input {
	width: 1% !important;
}

.inner-heading2 {
	padding: 0 0px 18px 35px;
	border-bottom: 1px solid #ccc;
	margin: 0 0 20px 0;
}

.hedingsec {
	width: 100%;
	display: inline-block;
}

.inner-hedingsec {
	border-bottom: 1px solid #CCC;
	padding-bottom: 15px;
}

.inner-hedingsec h2 {
	float: left;
	margin: 0;
}

.hedingsec h2 {
	float: left;
	margin: 0;
}

.hedingsec form {
	width: 32%;
	float: right;
}

.hedingsec form input[type="text"] {
	width: 74%;
	padding: 0 5px;
	border: 1px solid #c6c6c6;
	box-shadow: none;
	height: 34px;
	font-size: 14px;
	color: #343434;
	border-radius: 5px 0 0 5px;
	float: left;
}

.hedingsec form input[type="submit"] {
	background-color: #ff7c00;
	border: 1px solid #dc6c02;
	box-shadow: none;
	font-size: 14px;
	color: #fff;
	border-radius: 0 5px 5px 0;
	padding: 6px 12px;
}

.hedingsec form input[type="submit"]:hover {
	background: #e55b39;
	border: 1px solid #b13b1f;
}

.plan-table .carrier-data {
	padding: 0px 0;
}

.plan-table .form-btn .cancel-btn {
	margin: 0 20px;
}

.chng-stats {
	
}

.chng-stats {
	width: 39%;
	float: right;
}

.chng-stats form label {
	float: left;
	font-size: 15px;
	color: #000;
	padding: 5px;
	margin: 0;
	width: 41%;
}

.chng-stats form select {
	width: 59%;
	padding: 0 5px;
	border: 1px solid #c6c6c6;
	box-shadow: none;
	height: 34px;
	font-size: 14px;
	color: #343434;
	border-radius: 5px 0 0 5px;
	float: left;
}

