.body-con {
	width: 100%;
	float: left;
	margin-bottom: 25px;
	min-height:365px;
}

.body-bg {
	background: #e3e3e3;
	padding: 25px;
	min-height:325px;
}

.body-leftbg {
	width: 344px;
	float: left;
	margin-top: 10px;
}

.leftcon-bg {
	background: #fff;
	border: 1px solid #e6e6e6;
	border-radius: 5px;
	margin-left: 10px;
}

.leftcon-bg h2 {
	background: url(../images/noti-icon.png) 12px center no-repeat #F16527;
	border-radius: 5px 5px 0 0;
	margin: 0;
	font-size: 20px;
	line-height: 45px;
	font-weight: normal;
	color: #fff;
	padding-left: 42px;
}

.leftcon-bg ul {
	padding: 0;
	margin: 0;
	list-style: none;
}

.leftcon-bg ul li {
	border-bottom: 1px solid #e6e6e6;
	padding: 10px 12px;
	font-size: 15px;
	line-height: 20px;
	color: #5b5b5d;
}

.leftcon-bg ul li a {
	font-size: 15px;
	line-height: 20px;
	color: #5b5b5d;
}

.leftcon-bg ul li a:hover {
	color: #f16527;
}

.leftcon-bg ul li:last-child {
	border-bottom: none;
}

.mytask {
	margin-top: 35px;
}

.mytask h2 {
	background: url(../images/task-icon.png) 12px center no-repeat #F16527;
	border-radius: 5px 5px 0 0;
	margin: 0;
	font-size: 20px;
	line-height: 45px;
	font-weight: normal;
	color: #fff;
	padding-left: 48px;
}

.rightcon-bg {
	width: 578px;
	float: right;
	/* background: url(../images/home-logo-1.png) center no-repeat #fff; */
	background: #fff;
	border: 1px solid #e6e6e6;
	border-radius: 5px;
	text-align: center;
	min-height: 612px;
	margin-top: 10px;
	margin-right: 10px;
}
.rightcon-bg .home-logo{
	
	height: 220px;
	text-align: center;
	padding-top: 30px;
	display: inline-block;
}

.rightcon-bg h2 {
	font-size: 30px;
	line-height: 36px;
	color: #010101;
	margin: 62px 0 0 0;
	font-weight: normal;
	padding: 0 5%;
}

.rightcon-bg h3 {
	font-size: 26px;
	line-height: 28px;
	color: #8a8d8f;
	margin-top: 75px;
}

.rightcon-bg ul {
	padding: 0;
	margin-top: 25px;
	list-style: none;
}

.rightcon-bg ul li {
	font-size: 16px;
	line-height: 36px;
	color: #1e1e1e;
}

.rightcon-bg ul li a {
	font-size: 16px;
	line-height: 36px;
	color: #1e1e1e;
}

footer {
	width: 100%;
	float: left;
	background: #f1eded;
	border-top: 3px solid #ff8000;
	padding: 10px 0 15px;
}

footer p {
	font-size: 14px;
	line-height: 16px;
	color: #3d3d3d;
	margin-top: 8px;
	margin-bottom: 0;
}

footer p span {
	margin: 0 5px;
}

.footer-left {
	float: left;
}

.footer-right {
	float: right;
	margin-top: 10px
}

ul.footer-link {
	float: left;
	padding: 0;
	margin: 0;
	list-style: none;
}

ul.footer-link li {
	float: left;
	font-size: 14px;
	line-height: 16px;
	color: #3d3d3d;
}

ul.footer-link li span {
	margin: 0 5px;
}

ul.footer-link li a {
	font-size: 14px;
	line-height: 16px;
	color: #3d3d3d;
}

ul.social-img {
	float: left;
	padding: 0;
	margin-top: -6px;
	list-style: none;
	margin-left: 10px;
}

ul.social-img li {
	float: left;
	margin: 0 2px;
}

h2.inner-heading {
	font-size: 26px;
	line-height: 28px;
	color: #343434;
	margin: 0;
	margin-bottom: 20px;
}

.carrier-data {
	background: #fff;
	border: 1px solid #e6e6e6;
	border-radius: 5px;
	padding: 20px 0;
}

.carrier-field {
	width: 44%;
	float: left;
	margin-left: 4%;
	margin-bottom: 15px;
}

.carrier-field-email {
	margin-top: 25px;
}

.carrier-field label {
	width: 100%;
	float: left;
	font-weight: normal;
	font-size: 17px;
	margin-bottom: 10px;
}

.carrier-field label.email-noti {
	float: left;
	width: auto;
	margin-right: 20px;
}

.carrier-field span {
	width: 100%;
	float: left;
}

.carrier-field span.radio-left {
	float: left;
	width: auto;
	margin: 2px 0 0 8%;
}

.carrier-field span.radio-left input[type='radio'] {
	float: left;
	margin-right: 10px;
}

.carrier-field span.radio-left p {
	float: left;
	margin-right: 20px;
}

.form-control {
	/* background-color: #fff !important; */
	border-color: #c6c6c6;
	box-shadow: none;
	height: 40px;
	font-size: 14px;   /* change reverted */
	/* color: #ffff; */
	border-radius: 5px;
}

.upload-input {
	background-color: #fff;
	border: 1px solid #c6c6c6;
	box-shadow: none;
	font-size: 14px;
	color: #343434;
	border-radius: 5px;
	width: 100%;
	padding: 6px 12px;
}

.form-btn {
	width: 100%;
	float: left;
	text-align: center;
	margin: 25px 0;
}

.save-btn {
	display: inline-block;
	background: #ff7c00;
	font-size: 17px;
	line-height: 36px;
	color: #fff;
	padding: 0 30px;
	border: 1px solid #dc6c02;
	border-radius: 5px;
}

.save-btn:hover {
	background: #F16527;
	border: 1px solid #b13b1f;
	text-decoration: none;
	color: #FFF;
}

.cancel-btn {
	display: inline-block;
	background: #FF7C00;
	font-size: 17px;
	line-height: 36px;
	color: #fff;
	padding: 0 30px;
	border: 1px solid #dc6c02;
	border-radius: 5px;
	margin-left: 20px;
}

.cancel-btn:hover {
	background: #F16527;
	border: 1px solid #b13b1f;
	color: #fff;
	text-decoration: none;
}

.carrier-field span.edit-field {
	width: 100%;
	float: left;
	background: #f1eded;
	border: 1px solid #c6c6c6;
	font-size: 14px;
	line-height: 40px;
	height: 40px;
	color: #343434;
	border-radius: 5px;
	padding: 0 12px;
}

.delete-btn {
	display: inline-block;
	background: #ff7c00;
	font-size: 17px;
	line-height: 36px;
	color: #fff;
	padding: 0 30px;
	border: 1px solid #dc6c02;
	border-radius: 5px;
	margin-left: 20px;
}

.delete-btn:hover {
	background: #F16527;
	border: 1px solid #b13b1f;
}

.runing-btn {
	display: inline-block;
	background: #ff7c00;
	font-size: 17px;
	line-height: 36px;
	color: #fff;
	padding: 0 30px;
	border: 1px solid #dc6c02;
	border-radius: 5px;
}

.runing-btn:hover {
	background: #F16527;
	border: 1px solid #b13b1f;
}

