body {
	font-family: Arial, Helvetica, sans-serif;
}

.clear20 {
	width: 100%;
	height: 20px;
	margin: 0;
	padding: 0;
	float: left;
}
a {
	cursor: pointer;
}

a:focus, div:focus, img:focus {
	outline: none;
	outline-style: none;
	box-shadow: none;
	border-color: transparent;
}

input:focus, select:focus, textarea:focus{
	outline: none !important;
    box-shadow: 0 0 10px #719ECE;
}

.maincont {
	width: 1140px;
}

header {
	width: 100%;
	float: left;
	margin-top: 20px;
}

.border_bottom {
	border-bottom: 3px solid #eaeaea;
}

.logopart {
	float: left;
	margin-bottom: 15px;
}

.logopart p {
	color: #63676a;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 20px;
	padding-top: 10px;
}

.top-right {
	float: right;
	margin-top: 23px;
	text-align: right;
}

.top-right p {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	line-height: 36px;
	color: #63676a;
	display: inline-block;
	margin-right: 27px;
}

.top-right p span {
	color: #ffb200;
}

.top-right p a {
	color: #ffb200;
}

.top-right a.logout {
	background: #e55b39;
	font-size: 16px;
	line-height: 36px;
	color: #fff;
	font-family: Arial, Helvetica, sans-serif;
	font-weight: normal;
	display: inline-block;
	border-radius: 5px;
	padding: 0 24px;
}

.top-right a.logout:hover {
	background: #c64323;
	text-decoration: none;
}

.header-bottom {
	width: 100%;
	float: left;
	margin-top: 25px;
	margin-bottom: 25px;
}

.header-bottom ul {
	float: right;
	list-style: none;
	padding: 0;
	margin: 0;
}

.header-bottom ul li {
	float: left;
	font-size: 14px;
	line-height: 16px;
	color: #343434;
	margin-right: 15px;
}

.header-bottom ul li:last-child {
	margin-right: 0;
}

.notification {
	background: url(../images/noti-icon2.png) 0 0 no-repeat;
	padding-left: 30px;
}

.preferences {
	background: url(../images/preferences-icon.png) 0 0 no-repeat;
	padding-left: 30px;
}

.workicon {
	background: url(../images/workicon.png) 0 0 no-repeat;
	padding-left: 33px;
}

.header-bottom ul li a {
	font-size: 14px;
	line-height: 22px;
	color: #343434;
}

.header-bottom ul li a:hover {
	color: #F16527;
}

.header-bottom ul li a.active {
	color: #F16527;
}

.menu {
	width: 100%;
	float: left;
	text-align: center;
}
